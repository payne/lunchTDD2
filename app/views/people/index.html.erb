<p id="notice"><%= notice %></p>

<h1>Listing People</h1>

<table border=2>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>email</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @people.each do |p| %>
      <tr>
        <td><%= p.first_name %></td>
        <td><%= p.last_name %></td>
        <td><%= p.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

